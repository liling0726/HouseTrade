<?xml version="1.0" encoding="utf-8"?>
<search-condition-groups>
	<search-condition-group scgname="售价">
		<search-condition>
			<scname>30万以下</scname>
			<scid>p1</scid>
			<scsql><![CDATA[house.salePrice<30]]></scsql>
		</search-condition>
		<search-condition>
			<scname>30万-60万</scname>
			<scid>p2</scid>
			<scsql><![CDATA[house.salePrice>=30 and house.salePrice<=60]]></scsql>
		</search-condition>
		<search-condition>
			<scname>60万-100万</scname>
			<scid>p3</scid>
			<scsql><![CDATA[house.salePrice>=60 and house.salePrice<=100]]></scsql>
		</search-condition>
		<search-condition>
			<scname>100万-150万</scname>
			<scid>p4</scid>
			<scsql><![CDATA[house.salePrice>=100 and house.salePrice<=150]]></scsql>
		</search-condition>
		<search-condition>
			<scname>150万-200万</scname>
			<scid>p5</scid>
			<scsql><![CDATA[house.salePrice>=150 and house.salePrice<=200]]></scsql>
		</search-condition>
		<search-condition>
			<scname>200万以上</scname>
			<scid>p6</scid>
			<scsql><![CDATA[house.salePrice>200]]></scsql>
		</search-condition>
	</search-condition-group>
	
